{
  "name": "my-lib-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:test": "ng serve test-packer",
    "start:prod": "ng serve prod-packer",
    "install": "npm install file:./dist/my-lib",
    "concat": "node concat.js",
    "build": "ng build",
    "build:watch": "npm run install && ng build my-lib --watch",
    "build:dev": "ng build my-lib && npm run install && ng build prod-packer --output-hashing=none && npm run concat",
    "sandbox:dev": "ng build my-lib && npm run install && ng build test-packer",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "compodoc": "npx compodoc -p tsconfig.doc.json -d docs -o -s -nTemplate-Widget -w",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/elements": "^14.0.2",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@types/estree": "^0.0.47",
    "my-lib": "file:dist/my-lib",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "14.0.0",
    "@angular-devkit/build-angular": "^14.0.2",
    "@angular/cli": "~14.0.2",
    "@angular/compiler-cli": "^14.0.0",
    "@compodoc/compodoc": "^1.1.19",
    "@ngtools/webpack": "^14.0.2",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "concat-files": "^0.1.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "file-merge-webpack-plugin": "^1.1.0",
    "jasmine-core": "~4.1.0",
    "javascript-obfuscator": "2.8.0 || 3.0.0 || 4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "minicat": "^1.0.0",
    "ng-packagr": "^14.0.0",
    "prettier": "^2.7.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "~4.7.2",
    "webpack-cli": "^4.10.0",
    "webpack-obfuscator": "^3.5.1"
  }
}
